{
	"name": "netiq-idm-tracetool",
	"displayName": "Tracetool",
	"description": "NetIQ IDM advanced trace tool",
	"publisher": "GenLan d.o.o.",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "tracetool-sidebar-view",
					"title": "NetIQ IDM Tracetool",
					"icon": "media/icons/tools.svg"
				}
			]
		},
		"views": {
			"tracetool-sidebar-view": [
				{
					"type": "tree",
					"id": "trace-navigation",
					"name": "Trace Navigation"
				},
				{
					"type": "tree",
					"id": "trace-policyList",
					"name": "Trace Policy List"
				},
				{
					"type": "tree",
					"id": "trace-transactionList",
					"name": "Trace Transaction List"
				},
				{
					"type": "tree",
					"id": "trace-bookmarks",
					"name": "Bookmarks"
				},
				{
					"type": "webview",
					"id": "trace-stats",
					"name": "Stats"
				}
			]
		},
		"commands": [
			{
				"command": "tracetool.addBookmark",
				"title": "Add bookmark",
				"category": "Tracetool"
			},
			{
				"command": "tracetool.previousOccurance",
				"title": "Previous occurance",
				"icon": "media/icons/arrowUp.svg",
				"category": "Tracetool"
			},
			{
				"command": "tracetool.nextOccurance",
				"title": "Next occurance",
				"icon": "media/icons/arrowDown.svg",
				"category": "Tracetool"
			},
			{
				"command": "tracetool.eventOccurance",
				"title": "Current event first occurance",
				"icon": "media/icons/arrowRight.svg",
				"category": "Tracetool"
			},
			{
				"command": "tracetool.goToTransaction",
				"title": "Go to transaction",
				"icon": "media/icons/arrowRight.svg",
				"category": "Tracetool"
			},
			{
				"command": "tracetool.goToTransactionBottom",
				"title": "Go to transaction bottom",
				"icon": "media/icons/arrowBottom.svg",
				"category": "Tracetool"
			}
		],
		"taskDefinitions": [],
		"menus": {
			"view/title": [],
			"view/item/context": [
				{
					"command": "tracetool.nextOccurance",
					"group": "inline@1",
					"when": "view == trace-navigation || view == trace-policyList"
				},
				{
					"command": "tracetool.previousOccurance",
					"group": "inline",
					"when": "view == trace-navigation || view == trace-policyList"
				},
				{
					"command": "tracetool.eventOccurance",
					"group": "inline",
					"when": "view == trace-navigation || view == trace-policyList"
				},
				{
					"command": "tracetool.goToTransaction",
					"group": "inline",
					"when": "view == trace-transactionList"
				},
				{
					"command": "tracetool.goToTransactionBottom",
					"group": "inline@1",
					"when": "view == trace-transactionList"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.20.6",
		"@types/serialport": "^8.0.1",
		"@types/vscode": "^1.53.0",
		"@types/xmldom": "^0.1.34",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"require": "^2.4.20",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"bindings": "^1.5.0",
		"xmldom": "^0.6.0",
		"xpath": "^0.0.34",
		"yarn": "^1.22.10"
	}
}
